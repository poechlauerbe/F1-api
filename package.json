{
  "name": "my-f1-app",
  "version": "0.3.0",
  "description": "My small F1 app. Used API: [OpenF1](https://openf1.org/)",
  "main": "server.js",
  "scripts": {
    "start": "node ./server 2>&1 | tee -a ./log/stderr_$(date +'%Y-%m-%d-%H').log",
    "dev": "mkdir -p log && nodemon server.js 2>&1 | tee -a ./log/stderr_$(date +'%Y-%m-%d-%H').log"
  },
  "keywords": [
    "F1",
    "formula1",
    "App",
    "express",
    "node.js",
    "ejs"
  ],
  "author": "poechlauerbe",
  "license": "MIT",
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  }
}
